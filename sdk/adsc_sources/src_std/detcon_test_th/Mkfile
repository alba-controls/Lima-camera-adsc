CC = cc
CFLAGS = $(CFLAGS_OS) -D$(OSTYPE)
IOFLAGS = 
OEXTS = detcon_test_th.o
OBS  = 
OBJS = $(OEXTS) $(OBS) ../../lib/$(OSTYPE)/libdetcon_th.a ../../lib/$(OSTYPE)/libauxlib.a 
DD = ../../bin/$(OSTYPE)

PRCMD = pr

$(DD)/detcon_test_th : $(OBJS)
	$(CC) $(CFLAGS) $(OBJS) $(LDFLAGS) $(IOFLAGS) -o $(DD)/detcon_test_th

$(OEXTS) : ../incl/ccdconv.h 

clean :	$(OEXTS) $(OBS)
	rm $?

print :	*.c *.h Makefile
	$(PRCMD) $?
	touch print
